<!ELEMENT default (uri)>

<!ELEMENT rollover (uri)>

<!ELEMENT disabled (uri)>

<!ELEMENT pressed (uri)>

<!ELEMENT selected (uri)>

<!ELEMENT uri (#PCDATA)>